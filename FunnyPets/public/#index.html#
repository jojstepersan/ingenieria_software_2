        function crearUsuarioNuevo() {
            email=document.getElementById("email").value;
            console.log(email);
            password=document.getElementById("pass").value;
            firebase.auth().createUserWithEmailAndPassword(email, password).catch(function (error) {
                // Handle Errors here.
